{
	"root": false,
	"extends": "//",
	"css": {
		"parser": {
			"tailwindDirectives": true
		}
	},
	"linter": {
		"rules": {
			"style": {
				"useFilenamingConvention": "off",
				"noDefaultExport": "off"
			},
			"suspicious": {
				"noArrayIndexKey": "off",
				"noTemplateCurlyInString": "off"
			}
		}
	}
}
